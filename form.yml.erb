<%-
  groups = OodSupport::User.new.groups.sort_by(&:id).tap { |groups|
    groups.unshift(groups.delete(OodSupport::Process.group))
  }.map(&:name).grep(/^P./)
-%>
---
cluster: "pc-dev"
form:
  - account
  - bc_num_hours
  - version
attributes:
  account:
    label: "Project"
    widget: select
    options:
      <%- groups.each do |group| %>
      - "<%= group %>"
    <%- end %>
version:
  widget: "select"
  label: "Codeserver Version"
  options:
    - ["3.10", "3.10.2"]
    - ["4.12", "4.12.0"]

working_dir: ""
